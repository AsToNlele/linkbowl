{
  "name": "linkbowl",
  "version": "1.0.0",
  "description": "Service similar to linktree",
  "scripts": {
    "client": "cd frontend && npm run dev",
    "server": "cd backend && npm run develop",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-frontend": "cd frontend && npm install",
    "install-backend": "cd backend && npm install",
    "install-all": "concurrently \"npm install \" \"npm run install-frontend\" \"npm run install-backend\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AsToNlele/linkbowl.git"
  },
  "author": "Alexandr Celakovsky",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AsToNlele/linkbowl/issues"
  },
  "homepage": "https://github.com/AsToNlele/linkbowl#readme",
  "dependencies": {
    "concurrently": "^6.2.1"
  }
}
